[mTween]
Measure=Script
ScriptFile=#@#LuaTween/Main.lua
TweenGroup=Tweenable
Tween0=Single | Variable | TweenNode1 | 0 | 1 | #Style.Time1#
Tween1=Single | Variable | TweenNode2 | 0 | 1 | #Style.Time2#
Optional0=Easing InOutExpo
Optional1=Easing InOutExpo
UpdateDivider=1

[Image2]
MEter=Image
X=(#SCREENAREAWIDTH#/2-300*#TweenNode1#/2-[String:W]/2+#TweenNode2#*300)
Y=(#SCREENAREAHEIGHT#/2-300*#TweenNode1#/2)
W=(300*#TweenNode1#)
H=(300*#TweenNode1#)
ImageAlpha=(50*(1-#TweenNode2#))
DynamicVariables=1
ImageName=#SKINSPATH#..\CoreData\Keylaunch\IconCache\#Sec.Icon#.png
Greyscale=#Greyscale#

[Image]
MEter=Image
X=(#SCREENAREAWIDTH#/2-256*#TweenNode1#/2-[String:W]/2+#TweenNode2#*300)
Y=(#SCREENAREAHEIGHT#/2-256*#TweenNode1#/2)
W=(256*#TweenNode1#)
H=(256*#TweenNode1#)
ImageAlpha=(255*(1-#TweenNode2#))
DynamicVariables=1
ImageName=#SKINSPATH#..\CoreData\Keylaunch\IconCache\#Sec.Icon#.png
Greyscale=#Greyscale#

[String]
Meter=String
FontSize=(#Style.FontSize#)
X=(256+#CircSize#-128+10)r
Y=(256/2*#TweenNode1#)r
FontColor=#FontColor#,(255*(#TweenNode1#-0.5)/0.5-255*#TweenNode2#)
FontFace=#GlobalFont#
Text=#Sec.Name#
AntiAlias=1
StringAlign=LeftCenter
DynamicVariables=1
InlineSetting=Shadow | 0 | 0 | #Blur# | #Glowcolor#,#GlowOpacity#